{
  "compilerOptions": {
    "paths": {
      "@/*": ["./*"],
      "@/components/*": ["./components/*"],
      "@/lib/*": ["./lib/*"],
      "@/hooks/*": ["./hooks/*"],
      "@/utils/*": ["./utils/*"]
    },
    "skipLibCheck": true
  },
  "exclude": ["node_modules"],
  "typeAcquisition": {
    "enable": false
  }
}

